        <mxCell id="tH8JoVMTj8ZfuamFfaAy-67" value="&lt;font style=&quot;font-size: 18px;&quot;&gt;Ненормализованная БД:&lt;/font&gt;" style="text;html=1;align=center;verticalAlign=middle;whiteSpace=wrap;rounded=0;" vertex="1" parent="1">
